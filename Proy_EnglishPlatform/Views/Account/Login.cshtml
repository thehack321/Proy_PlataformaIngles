@model Proy_EnglishPlatform.Models.LoginViewModel

@{
    ViewBag.Title = "Log in";
}

<head>

    <link href="~/css/login.css" rel="stylesheet">
    <script type="text/javascript" src="assets/js/jquery.js"></script>
    <script type="text/javascript" src="assets/js/bootstrap.js"></script>
    <script type="text/javascript" src="assets/js/login/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="assets/js/login/jquery.validate.js"></script>
    <script type="text/javascript" src="assets/js/login/jquery.watermark.min.js"></script>
    <script type="text/javascript" src="assets/js/login/login.js"></script>
    
</head>

<div class=" container">
    <div class="row">
        <div class="span4"></div>
        <div class="span4 middle">
            <div class="loginbox">
                <h2>Ingresar</h2>
                <form action="#" method="post" id="subForm" class="theme-form sign-up-form">
                    <input type="search" name="Username" title="USERNAME" id="name-field" placeholder="Usuario" class="name sign-up-form">
                    <br>
                    <br>
                    <input type="password" name="Password" title="PASSWORD" id="email-field" placeholder="Contraseña" class="sign-up-form">
                    <br>
                    <br>
                    <br>
                    <input type="submit" value="Entrar">
                    <br>
                    <br>
                    <a href="#"><p>Olvidaste tu contraseña?</p></a>
                    <br>
                </form>
            </div>
        </div>
        <div class="span4"></div>
    </div>
</div>